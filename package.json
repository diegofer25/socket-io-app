{
  "name": "socket-io-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "npm run clear:dist && npm run compile && node dist/index.js",
    "start": "npm run build && node dist/index.js",
    "build": "npm run clear && npm i && npm run compile",
    "clear": "npm run clear:modules && npm run clear:dist",
    "clear:modules": "rm -rf node_modules",
    "clear:dist": "rm -rf dist",
    "compile": "tsc",
    "remote": "gcloud compute ssh node-app --zone us-central1-a",
    "port-redirect": "sudo iptables -t nat -I PREROUTING -p tcp --dport 80 -j REDIRECT --to-port 8080"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.6",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^3.0.0"
  }
}
